page {
  width: 100%;
  box-sizing: border-box;
  padding: 30rpx 28rpx 30rpx 28rpx;
  background: #f5f5f5;
}
.container {
  width: 100%;
  height: 100%;
  background: white;
  border-radius: 20rpx;
  box-sizing: border-box;
  padding: 44rpx 32rpx;
  overflow-y: auto;
}
.first-title {
  height: 50rpx;
  line-height: 50rpx;
  font-size: 36rpx;
  color: #1CB975;
  font-weight: bold;
}
.custom-text {
  color: #222222;
  font-size: 32rpx;
  line-height: 44rpx;
  margin-top: 10rpx;
}
.second-title {
  color: #222222;
  font-size: 32rpx;
  /* height: 44rpx; */
  line-height: 44rpx;
  font-weight: bold;
}
.line {
  width: 100%;
  border-top: 2rpx dashed #F1F1F1;
  margin-top: 44rpx;
  margin-bottom: 50rpx;
}
.mg40{
  margin-top: 40rpx;
}
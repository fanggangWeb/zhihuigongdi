/* 顶部要固定定位   标题要居中   自定义按钮和标题要和右边微信原生的胶囊上下对齐 */
.nav-wrap {
  position: fixed;
  width: 100%;
  top: 0;
  background: #fff;
  color: #000;
  z-index: 9999999;
  overflow: hidden;
}
/* 标题要居中 */
.nav-title {
  position: absolute;
  text-align: center;
  max-width: 400rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  font-size: 30rpx;
  color: #2c2b2b;
  font-weight: normal;
}
/* 按钮包裹 */

.nav-capsule {
  display: flex;
  align-items: center;
  margin-left: 30rpx;
  width: 140rpx;
  justify-content: space-between;
  height: 64rpx;
  /* background: red; */
  /* border-radius: 20rpx;
  border: 1px solid #DADADA; */
}
.btn-con {
  padding: 4rpx 24rpx;
  display: flex;
  border-radius: 32rpx;
  border: 1px solid #DADADA;
  align-items: center;
  
}
.navbar-v-line {
  width: 1px;
  height: 30rpx;
  margin: 0 22rpx;
  background-color: #e5e5e5;
}

.back-pre, .back-home {
  width: 30rpx;
  height: 30rpx;
  cursor: pointer;
}
.nav-capsule .back-home {
  width: 30rpx;
  height: 30rpx;
  cursor: pointer;
}